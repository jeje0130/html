<sqlMap namespace="reply">

<insert id="insertReply" parameterClass="replyVO">
	insert into reply values(
		REPLY_RENUM_SEQ.nextval,
		#bonum#,
		#name#,
		#cont#,
		sysdate
	) 
	<selectKey keyProperty="renum" resultClass="int">
		select REPLY_RENUM_SEQ.currval from dual
	</selectKey>
</insert>

</sqlMap>