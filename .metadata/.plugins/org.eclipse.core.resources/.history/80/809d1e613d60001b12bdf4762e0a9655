<script>

$(function () {
	
	$('.box1 input[value=hidden]').on('click', function () {
		
		res = "";
		$('p:hidden').each(function(i) {
			res += $(this).text() + "<br>";
		})
		
		$('#hid').html(); // 위에 <br>태그가 있어서 html로 출력
	})
})

</script>
