<script>
$(function () {
	
	//request 객체를 생성
	xhr = new XMLHttpRequest();
	
	$('input:button').on('click', function () {
		alert('aa');
		//요청
		xhr.open("get", "../0202/member.jsp", true);
		xhr.send();
		
		//응답
		xhr.onreadystatechange = function () {
			
			if(this.readyState == 4 && this.status == 200){
				
				res = JSON.parse(this.responseText);
				
				
			}
		}
	})
})
</script>
