//arr = new Array("사과","복숭아","참외","포도","자두")
arr = ["사과","복숭아","참외","포도","자두"]

function proc1() {
	item = "포도";
	idx = arr.indexOf(item);
	
	alert(`${item}는   ${idx} 번째 있습니다`)
}
