<script>
	var circle = { 
			//속성
			r : 5,
			//메소드
			getA : function() { //넓이
				a = (this.r * this.r) * Math.PI;
				return a;
			},
			
			getB : function() { //둘레
				b = (2 * Math.PI) * this.r;
				return b;
			}
	}
	
	function proc1() {
		
		res += `넓이 : ${circle.getA()} <br>`;
		res += `둘레 : ${circle.getB()};
		
		result = document.querySelector("#select1");
		result.innerHTML = res;
		
	}
	
	function proc2() {
		
	}
	

</script>
