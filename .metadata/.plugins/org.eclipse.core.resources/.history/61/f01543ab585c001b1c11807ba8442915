<script>
//준비
$(function () {
	
	$('input:button').on('click', function () {
	
		//p태그 생성
		p = $('<p class="aa" onclick="proc1()">HELLO</p>');
		
		//result1에 p태그를 추가
		$(p).appendTo($('#result1'));
	})
	
	
})
</script>
