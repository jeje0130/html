<script>

function proc1() {
	
	now1 = new Date();
	now2 = new Date(12340098777);
	now3 = new Date("2020. 04. 20")
	now4 = new Date(2020, 04-1, 20) // month 는 0부터 시작해서 4월이지만 may로 나옴
	
	//출력모양
	res = now1 + "<br>";
	res += now2 + "<br>";
	res += now3 + "<br>";
	res += now4 + "<br>";
	
	result = document.getElementById("result1");
	result.innerHTML = res;
}

</script>
<style>
</style>
</head>
<body>

<div class="box">
	* new Date //현재 날짜와 시간 <br>
	* new Date(milliseconds) //1970/01/01 이후의 밀리초<br>
	* new Date(dateString)<br>
	* new Date(year, month, date[, hours[, minutes[, seconds[,ms]]]])<br>
	
 <br><br>
	
<input type="button" value="확인" onclick="proc1()">

<div id="result1"></div>
